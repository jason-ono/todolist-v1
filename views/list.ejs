<%- include("header") -%>
<div class="box" id="heading">
  <h1>It's <%= listTitle %>.</h1>
</div>

<div class="box">
  <% for (var i=0; i<itemList.length;i++){ %>
  <div class="item">
    <input type="checkbox">
    <p> <%= itemList[i] %> </p>
  </div>
  <% } %>

  <form action="/" method="post">
    <input type="text" name="taskName" placeholder="New Task">
    <button type="submit" name="list" value=<%= listTitle %>>
      +
    </button>
  </form>
</div>
<%- include("footer") -%>